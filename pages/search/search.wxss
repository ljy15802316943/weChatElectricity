page {
  background: #ededed;
}
.searchInput {
  width: 100%;
  height: 110rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background: #EC2126;
  display: flex;
}
.searchInput .inoutBox {
  flex: 1;
  height: 100%;
  background: #ffffff;
  border-radius: 30rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
}
.searchInput .cancel {
  width: 100rpx;
  height: 100%;
  color: #666;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #ffffff;
  border-radius: 15rpx;
  margin: 0 10rpx;
}
.searchContent {
  width: 90%;
  height: auto;
  max-height: 86vh;
  background: #ffffff;
  border-radius: 5rpx;
  margin: auto;
  margin-top: 20rpx;
  overflow-y: scroll;
}
.searchContent .cotItem {
  width: 100%;
  padding: 15rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border-bottom: 1px solid #ededed;
}
.searchContent .beyondSearch {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #666;
  background: #ffffff;
}
